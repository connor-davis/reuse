{
    "name": "syllektis",
    "description": "This project was started due to the lack of actual value to collecting recyclable waste.",
    "version": "0.1.4",
    "private": true,
    "dependencies": {
        "@reduxjs/toolkit": "^1.5.0",
        "@testing-library/jest-dom": "^5.11.4",
        "@testing-library/react": "^11.1.0",
        "@testing-library/user-event": "^12.1.10",
        "axios": "^0.21.1",
        "bootstrap": "^4.6.0",
        "bootswatch": "^4.6.0",
        "electron-build": "^0.0.3",
        "electron-dialogs": "^1.4.0",
        "electron-log": "^4.3.2",
        "electron-updater": "^4.3.8",
        "file-saver": "^2.0.5",
        "levelup": "^4.4.0",
        "moment": "^2.29.1",
        "node-sass": "^5.0.0",
        "pouchdb": "^7.2.2",
        "pouchdb-json": "^7.2.2",
        "pouchdb-mapreduce": "^7.2.2",
        "pouchdb-md5": "^7.2.2",
        "pouchdb-merge": "^7.2.2",
        "pouchdb-replication": "^7.2.2",
        "pouchdb-upsert": "^2.2.0",
        "react": "^17.0.1",
        "react-dom": "^17.0.1",
        "react-icons": "^4.2.0",
        "react-redux": "^7.2.2",
        "react-router-dom": "^5.2.0",
        "react-scripts": "4.0.3",
        "reactstrap": "^8.9.0",
        "redux": "^4.0.5",
        "redux-axios-middleware": "^4.0.1",
        "redux-persist": "^6.0.0",
        "uuid": "^8.3.2",
        "web-vitals": "^1.0.1",
        "xlsx": "^0.16.9"
    },
    "main": "public/electron.js",
    "homepage": "./",
    "scripts": {
        "start": "react-scripts start",
        "build": "react-scripts build",
        "test": "react-scripts test --env=jsdom",
        "eject": "react-scripts eject",
        "electron": "electron build/electron.js",
        "electron-dev": "set ELECTRON_START_URL=http://localhost:5000 && electron .",
        "dev": "nf start",
        "prod": "npm run build && npm run electron",
        "pack": "electron-builder --dir",
        "dist": "electron-builder --win",
        "gh-publish-win": "npm run build && electron-builder --win -p always",
        "gh-publish-mac": "npm run build && electron-builder --mac -p always"
    },
    "eslintConfig": {
        "extends": [
            "react-app",
            "react-app/jest"
        ]
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "repository": "git@github.com:connor-davis/syllektis.git",
    "author": "Connor Davis <19422@kloofhigh.co.za>",
    "devDependencies": {
        "electron": "^12.0.1",
        "electron-builder": "^22.10.5",
        "foreman": "^3.0.1"
    },
    "build": {
        "appId": "tech.connordavis.syllektis",
        "compression": "maximum",
        "productName": "Syllektis",
        "target": "NSIS",
        "win": {
            "icon": "https://cdn2.iconfinder.com/data/icons/designer-skills/128/react-256.png"
        },
        "files": [
            "build/**/*",
            "node_modules/electron-log",
            "node_modules/electron-updater",
            "node_modules/electron-dialogs"
        ],
        "nsis": {
            "allowToChangeInstallationDirectory": true,
            "oneClick": false
        }
    }
}
